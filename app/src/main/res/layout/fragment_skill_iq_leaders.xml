<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SkillIQLeadersFragment" >

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/skillIQLeadersList"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:listitem="@layout/skill_iq_leader_item" />

    <ProgressBar
        android:id="@+id/skillIQLeadersProgress"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/colorPrimary"
        android:progressBackgroundTint="#970E0E"
        app:layout_constraintBottom_toBottomOf="@+id/skillIQLeadersList"
        app:layout_constraintEnd_toEndOf="@+id/skillIQLeadersList"
        app:layout_constraintStart_toStartOf="@+id/skillIQLeadersList"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>